<script>
  $(function(){
    window.onresize = function(){
      if($(parent.window).width() <= 1280){
        $('#ad_right').hide();
        $('#ad_left').hide();
      } else {
          $('#ad_right').show();
          $('#ad_left').show();
      }
    };
      if ($(parent.window).width() >= 1280) {
          $('#ad_right').show();
          $('#ad_left').show();
      }
  });

</script>

<script type="text/javascript" src="//g.csdnimg.cn/baidu-search/1.0.8/baidu-search.js"></script>
<script  type="text/javascript">
  if (window.csdn === undefined) {
    window.csdn = {};
  }
  if (typeof Array.prototype.filter != "function") {
    Array.prototype.filter = function (fn, context) {
      var arr = [];
      if (typeof fn === "function") {
         for (var k = 0, length = this.length; k < length; k++) {
            fn.call(context, this[k], k, this) && arr.push(this[k]);
         }
      }
      return arr;
    };
  }
  var keyword = encodeURI("BADAO_LIUMANG_QIZHI的留言板");
  // 重新定义百度Cas对象初始化操作
  csdn.afterCasInit=function (keyword,cse) {
    csdn.baiduSearch(keyword, function(data){
      var result = diff(data)
      showResult(result);
    })
  }
  function getLinkId (link) {
    if (!String.prototype.endsWith) {
      String.prototype.endsWith = function(search, this_len) {
        if (this_len === undefined || this_len > this.length) {
          this_len = this.length;
        }
        return this.substring(this_len - search.length, this_len) === search;
      };
    }
    if (!!~link.indexOf('?')) {
      link = link.split('?')[0]
    } else if (!!~link.indexOf('#')) {
      link = link.split('#')[0]
    }
    link = link.endsWith('/') ? link.slice(0, link.length - 1) : link
    return link.split('/').pop()
  }
  // 百度搜索跟现有数组去重
  function diff (data) {
    var currentIdList = []
    var elLi = $(".post_feed_wrap .bbs_feed")
    for (var i = 0; i < elLi.length; i++) {
      currentIdList.push(getLinkId(elLi.eq(i).find('a').attr('href')))
    }
    var finalData = data.filter(function (item) {
      return currentIdList.indexOf(getLinkId(item.linkUrl)) === -1
    })
    return finalData
  }
  function showResult(data){
    var elLi = $(".post_feed_wrap .bbs_feed");
    var dataLength = elLi.length;
    baiduLenght = data.length;
    var html = "";
    for (var i = 0; i < baiduLenght && i<10; i = i + 2) {
      var arr = [];
      if (data[i + 1]) {
        arr = convert(data.slice(i, i + 2), i);
      } else {
        arr = convert([data[i]], i);
      }
      if (baiduLenght > dataLength && i >= dataLength) {
        arr = convert(data.slice(i, baiduLenght), i);
        html = renderHtml(arr);
        elLi.eq(elLi.length - 1).after(html);
        break;
      } else {
        html = renderHtml(arr);
        elLi.eq(i).before(html);
      }
    }
  }


  function convert(arr, index) {
    var recom_utm = "";
    var popu = "";
    var newArr = [];
    for (var i = 0; i < arr.length; i++) {
      var local = 1 + index + i;
      newArr.push({
        linkUrl: arr[i].linkUrl,
        file_type_url:
          "//g.csdnimg.cn/pub-recom/img/type/unknow.svg",
        title: arr[i].title,
        created_at: formateDate(arr[i].dispTime),
        description: arr[i].abstract,
        strategy: "baidujs",
        mod: popu,
        utm_medium:
          recom_utm +
          checkChannel(arr[i].linkUrl) +
          "-baidujs-" +
          local +
          ".nonecase",
      });
    }
    return newArr;
  }
  function renderHtml(arr) {
    var s = "";
    $.each(arr, function( index, value ) {
      s += '<dl class="bbs_feed bbs_feed_baidu_box" data-report-view=\'{"mod":"popu_645","dest":"'+value['linkUrl']+'","strategy":"baidujs","extra":"{\\\"utm_medium\\\":\\\"distribute.pc_relevant_bbs_down.none-task-'+checkChannel(value['linkUrl'])+'-baidujs-'+(index+1)+'.nonecase\\\"}"}\'><dt><a href="'+value['linkUrl']+'" target="_blank" data-report-query="utm_medium=distribute.pc_relevant_bbs_down.none-task-'+checkChannel(value['linkUrl'])+'-baidujs-'+(index+1)+'.nonecase&depth_1-utm_source=distribute.pc_relevant_bbs_down.none-task-'+checkChannel(value['linkUrl'])+'-baidujs-'+(index+1)+'.nonecase"  data-report-click=\'{"mod":"popu_645","dest":"'+value['linkUrl']+'","strategy":"baidujs","extra":"{\\\"utm_medium\\\":\\\"distribute.pc_relevant_bbs_down.none-task-'+checkChannel(value['linkUrl'])+'-baidujs-'+(index+1)+'.nonecase\\\"}"}\'>'+value['title']+'</a></dt><dd><p>'+value['description']+'</p></dd></dl>'
    });
    return s;
  }
  function checkChannel(url) {
    var host = url.match(/^http(s)?:\/\/(.*?)\//)[0];
    var pathname = url.replace(host, "");
    if (host.indexOf("edu") > -1 && pathname.indexOf("detail") > -1) {
      return "edu";
    }
    if (host.indexOf("download") > -1 && pathname.indexOf("download") > -1) {
      return "download";
    }
    if (host.indexOf("bbs") > -1 && pathname.indexOf("topics") > -1) {
      return "bbs";
    }
    if (host.indexOf("ask") > -1 && pathname.indexOf("questions") > -1) {
      return "ask";
    }
    if (url.indexOf("article/details") > -1) {
      return "blog";
    }
    if (url.indexOf("www.csdn.net/gather") > -1) {
      return "gather";
    }
    return "other";
  }
  function formateDate(d) {
    var dArr = d.split("-");
    var nd = d;
    if(dArr.length===3 && Number(dArr[1]) < 10 ) {
      dArr[1] = "0" + dArr[1];
      nd = dArr.join("-");
    }
    if(dArr.length===3 && Number(dArr[2]) < 10 ) {
      dArr[2] = "0" + dArr[2];
      nd = dArr.join("-");
    }
    return d == nd ? d : nd;
  }

  $(function () {
    /** toolbar是否悬浮*/
    window.csdn.configuration_tool_parameterv({
        'need_first_suspend': true,//第一栏悬浮
        'need_little_suspend': false,//第二栏悬浮
        'little_tool_id': '' ,//第二栏id
        'little_need_insert_type': '',//第二栏需要模块类型
        need_change_function: function (flag) {
          window.csdn.navSecond.changeNavFix(flag)
        }
    })
  })

</script>
