      $(".csdn-bbs").show();
    })
  })

  //楼层底部吸地
  $(window).resize(function(){
    fixBottomboxScroll();
    fixBottombox();
    fixRightboxScroll();
    fixRightbox();
  });
  fixRightboxScroll();
  fixBottomboxScroll();
  fixBottombox();
  function fixBottomboxScroll() {
    $(document).scroll(function (event) {
      fixBottombox();
    })
  }
  function fixBottombox() {
      var d_left = $("#left-box").offset().left - $(window).scrollLeft();
      var d_el = $(".paginate_box");
      var aPoint = $(".paginate_box_point").offset().top;
      if (aPoint >= $(window).scrollTop() && aPoint < ($(window).scrollTop() + $(window).height())) {
          d_el.removeAttr("style");
      } else if (aPoint < $(window).scrollTop() && aPoint < ($(window).scrollTop() + $(window).height())){
          d_el.removeAttr("style");
      } else {
        d_el.css({
            "position":"fixed",
            "left":d_left+"px",
            "bottom":"0",
            "box-shadow":"0px -1px 1px 0px rgba(0,0,0,0.05)"
          });
      }
  }

  function fixRightboxScroll() {
    $(document).scroll(function (event) {
      fixRightbox();
    })
  }
  function fixRightbox() {
    var sTop = $(window).scrollTop();
    var sLeft = $(window).scrollLeft();
    var toolBarTop = $("#csdn-toolbar").height();
    var rightTop = sTop>toolBarTop?10:toolBarTop;
    var rightHeight = $("#rightList").height() + rightTop;
    var rightleft = $("#left-box").width() + $("#left-box").offset().left - sLeft;
    var winHeight = $(window).height();
    var docHeight = $(document).height();
    if(docHeight < rightHeight+85){
    } else if(winHeight > rightHeight) {
      if(sTop > 50){
        $("#rightList").css({
          "position": "fixed",
          "z-index": "99",
          "left": rightleft+"px",
          "width": "300px",
          "top": toolBarTop + "px"
        })
      } else {
        $("#rightList").removeAttr("style")
      }
    } else {
      if(sTop+winHeight>rightHeight){
        $("#rightList").css({
          "position": "fixed",
          "bottom": "0px",
          "z-index": "99",
          "left": rightleft+"px",
          "width": "300px",
          "top": "auto"
        })
      } else {
          $("#rightList").removeAttr("style")
      }
    }

  }



</script>
