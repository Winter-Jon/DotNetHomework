            $('em.jumpMenu').click(function() {
                var page = $(this).parent().find('input.page_inpt').val();
                if (page != 1) {
                    window.location.href = (window.location.pathname + '?page=' + page);
                } else {
                    window.location.href = window.location.pathname;
                }
            });
