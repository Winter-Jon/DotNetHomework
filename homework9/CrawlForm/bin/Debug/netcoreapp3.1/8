        <script>
            // 点赞
            function post_digg(topic_id,post_id,obj){
                window.csdn.loginBox.show();
                return false
                var r = check_user_status();
                if (r) {
                    $.ajax({
                        type:'PUT',
                        dataType: "json",
                        url:'/posts/'+post_id.toString()+'/digg',
                        data: {
                            topic_id: topic_id.toString()
                        },
                        success: function(json){
                            if(json.success){
                                $(obj).find("em").text("点赞 "+json.digg);
                                $(obj).find("span").addClass("save")
                            }else{
                                alert("您已经投过票了");
                            }
                        }
                    })
                }
            }

            function post_bury(topic_id,post_id,obj){
                var r = check_user_status();
                if(r){
                    $.ajax({
                        type:'PUT',
                        dataType: "json",
                        url:'/posts/'+post_id.toString()+'/bury',
                        data: {
                            topic_id: topic_id.toString()
                        },
                        success: function(json){
                            if(json.success){
                                $(obj).find("em").text(json.bury);
                            }else{
                                alert("您已经投过票了");
                            }
                        }
                    })
                }

            }


        </script>
